{
  "name": "vida",
  "version": "0.0.1",
  "appPath": "app/assets/",
  "dependencies": {
    "jquery-ui": "~1.10.4",
    "jquery": "~2.1.0",
    "bootstrap": "~3.1.1"

  },
  "devDependencies": {

  },
  "homepage": "https://github.com/kpse/vida",
  "authors": [
    "Suo Qin <kpsesuo@gmail.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "exportsOverride": {
    "jquery-ui": {
      ".": "ui/minified/jquery-ui.min.js",
      "css": "themes/base/minified/jquery-ui.min.css",
      "css/images": "themes/base/minified/images/*"
    },
    "jquery": {
      ".": ["dist/*.min.js{,.map}", "dist/*.min.map"]
    },
    "bootstrap": {
      "fonts": "dist/fonts/*.{eot,svg,ttf,woff}",
      "css": "dist/css/*.{min.css, map}",
      ".": "dist/js/*.min.js{,.map}"
    },
    "*": {
      ".": "*.min.js{,.map}"
    }
  }
}
