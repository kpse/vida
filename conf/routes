# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                       controllers.Application.index
GET        /cafe                   controllers.Application.cafe
GET        /changanye              controllers.Application.changanye


GET        /about                  controllers.Home.main
GET        /about_RED              controllers.Home.aboutRed
GET        /about_ninterior        controllers.Home.aboutN
GET        /work                   controllers.Home.aboutWork
GET        /contact                controllers.Home.aboutContact
GET        /team                   controllers.Home.aboutTeam

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file           controllers.Assets.at(path="/public", file)
